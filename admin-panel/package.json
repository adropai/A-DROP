{
  "name": "admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "optimize": "npm run lint && npm run build:prod",
    "cache:clear": "rm -rf .next",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:update": "jest --updateSnapshot",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright test --reporter=html",
    "playwright:install": "playwright install",
    "type-check": "tsc --noEmit",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed-permissions": "npx tsx prisma/seed-permissions.ts",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed && npm run db:seed-permissions",
    "cleanup": "node scripts/cleanup-auto-generated.js",
    "cleanup:aggressive": "node scripts/aggressive-cleanup.js",
    "destroyer": "node scripts/file-destroyer.js",
    "mega-destroyer": "node scripts/mega-destroyer.js",
    "dev:clean": "npm run cleanup && npm run dev",
    "dev:protected": "npm run mega-destroyer & npm run dev",
    "build:clean": "npm run cleanup && npm run build",
    "watch:cleanup": "node scripts/watch-cleanup.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@ant-design/plots": "^2.6.3",
    "@ant-design/pro-components": "^2.8.1",
    "@hookform/resolvers": "^5.2.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@tanstack/react-query": "^4.36.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/moment-jalaali": "^0.7.9",
    "@types/pdfkit": "^0.17.2",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "antd": "^5.22.2",
    "antd-jalali": "^2.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.16.4",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment-jalaali": "^0.10.4",
    "next": "14.2.15",
    "next-auth": "^4.24.11",
    "pdfkit": "^0.17.1",
    "persian-date": "^1.1.0",
    "prisma": "^6.13.0",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^13.5.0",
    "react-to-print": "^3.1.1",
    "recharts": "^3.1.2",
    "speakeasy": "^2.0.0",
    "swr": "^2.3.4",
    "zod": "^3.25.76",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.4.5",
    "@next/env": "^15.4.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.10",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "critters": "^0.0.23",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  }
}
